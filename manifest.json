{
    "name": "My Forecasts",
    "version": "1.0",
    "description": "Replace your new tab page with forecast",
    "manifest_version": 2,
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "chrome_url_overrides" : {
        "newtab": "index.html"
      },
      "icons": {"128": "ico.png" },
      "permissions": [        
        "chrome://favicon/", "history", "bookmarks"
      ]
  }